page{
	background-color: #f5f6fa;
}
.top{
	position: fixed;
	width: 100%;
	left: 0;
	z-index: 2;
}
.banner {
	width: 750rpx;
	height: 640rpx;
}
swiper{
	height: 640rpx;
}
swiper-item{
	height: 640rpx;
}
.banner image {
	width: 750rpx;
	height: 640rpx;
}
.part{
	padding: 0 30rpx;
}
.part .title{
	height: 114rpx;
	line-height: 114rpx;
	font-size: 32rpx;
	color: #16151b;
	font-weight: bold;
}
.part .title .first{
	margin-right: 16rpx;
	display: inline-block;
	height: 36rpx;
	line-height: 36rpx;
	border-radius: 4rpx;
	background-image: -webkit-gradient(linear, left top, right top, from(#fd6383) , to(#f73b62));
	background-image: -webkit-linear-gradient(left, #fd6383 , #f73b62);
	background-image: linear-gradient(to right, #fd6383 , #f73b62);
	font-size: 24rpx;
	color: #FFFFFF;
	padding: 0 12rpx;
	font-weight: 400;
}
.part .title text{
	vertical-align: middle;
}
.part .content{
	border-radius: 24rpx;
	background-color: #FFFFFF;
	padding: 0 45rpx;
	position: relative;
}
.part .content>image{
	height: 267rpx;
	position: absolute;
	left:-21rpx;
	top: 0;
	width: 42rpx;
}
.part .content .name{
	height: 100rpx;
	line-height: 100rpx;
	font-size: 32rpx;
	color:#16151b;
}
.part .content .name text{
	margin-right: 16rpx;
}
.part .content .first{
	display: inline-block;
	height: 45rpx;
	line-height: 45rpx;
	padding: 0 8rpx;
	border-radius: 8rpx;
	color:#0e8a39;
	border:1rpx solid #0e8a39;
	margin-right: 28rpx;
}
.part .content .address{
	line-height: 43rpx;
	font-size: 24rpx;
	color: #16151b;
	padding-bottom: 12rpx;
	border-bottom: 1rpx solid #f4f5f9;
}
button::after {
	border: none;
}
button{
	background-color: inherit;
	color: inherit;
	font-size: inherit;
	line-height: inherit;
}
.part .content .address image{
	width: 43rpx;
	height: 43rpx;
	vertical-align: middle;
}
.part .content .address text{
	vertical-align: middle;
}
.part .content .time{
	height: 56rpx;
	line-height: 56rpx;
	font-size: 24rpx;
	color:#6d6e80;
	padding-bottom: 12rpx;
}
.part .content .time image{
	width: 32rpx;
	height: 32rpx;
	vertical-align: middle;
}
.part .content .time text{
	vertical-align: middle;
	margin-left: 18rpx;
}
.sylla{
	margin-top: 26rpx;
	padding: 0 30rpx;
	padding-bottom: 100rpx;
}
.sylla .title{
	height: 70rpx;
	line-height: 70rpx;
	font-size: 24rpx;
	color: #16151b;
	font-weight: bold;
}
.sylla .entry .row{
	background-color: #FFFFFF;
	border-radius: 24rpx;
	margin-bottom: 20rpx;
	height: 184rpx;
}
.sylla .entry .row .left{
	line-height: 96rpx;
	width: 134rpx;
	text-align: center;
	margin-top: 19rpx;
	font-size: 56rpx;
	color: #efefef;
}
.sylla .entry .row .right{
	width: 510rpx;
	padding-right: 44rpx;
	margin-top: 19rpx;
}
.sylla .entry .row .right .desc{
	font-size: 28rpx;
	color:#16151b;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	height: 96rpx;
	line-height: 48rpx;
}
.sylla .entry .row .right .time{
	height: 68rpx;
	line-height: 68rpx;
}
.sylla .entry .row .right .time text{
	vertical-align: middle;
	font-size: 22rpx;
	color: #6d6e80;
}
.sylla .entry .row .right .time image{
	vertical-align: middle;
	margin-right: 17rpx;
	width: 21rpx;
	height: 21rpx;
}
.bottom{
	position: fixed;
	width: 690rpx;
	left: 0;
	height: 100rpx;
	line-height: 100rpx;
	bottom: 0;
	background-color: #FFFFFF;
	padding: 0 30rpx;
}
.bottom uni-icons{
	background-color: #f0f0f2;
	border-radius: 16rpx;
	padding: 20rpx;
}
.bottom .left{
	margin-right: 60rpx;
}
.bottom .left .row{
	display: inline-block;
	vertical-align: middle;
	line-height: initial;
	font-size: 22rpx;
	color: #6d6e80;
	text-align: center;
}
.bottom image{
	width: 36rpx;
	height: 36rpx;
}
.bottom .right{
}
.bottom .right text{
	display: inline-block;
	padding: 0 110rpx;
	height: 88rpx;
	line-height: 88rpx;
	vertical-align: middle;
	background-color: #3e55ff;
	border-radius: 16rpx;
	font-size: 32rpx;
	font-weight: bold;
	color:#FFFFFF;
}

