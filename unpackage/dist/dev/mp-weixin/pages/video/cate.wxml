<view><view class="top" style="{{('height:'+height+'px; padding-top:'+top+'px;')}}"><view class="title" style="{{('height:'+height+'px; line-height:'+height+'px; top:'+top+'px;')}}"><text>名师</text><view class="back" style="{{('height:'+height+'px;')}}"><uni-icons vue-id="4999686a-1" type="arrowthinleft" size="26" bind:__l="__l"></uni-icons></view></view><uni-data-picker vue-id="4999686a-2" localdata="{{gradeInfo}}" popup-title="请选择年级" data-event-opts="{{[['^change',[['onchange']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="right grade"><text>{{grade.text}}</text><view class="img"><image src="../../static/image/icon1.png"></image></view></view></uni-data-picker></view><view class="banner" style="{{('margin-top:'+all+'px;')}}"><swiper class="swiper" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}" indicator-dots="{{indicatorDots}}" indicator-color="{{indicatorColor}}" indicator-active-color="{{indicatorActiveColor}}"><block wx:for="{{banner}}" wx:for-item="v" wx:for-index="k"><block><swiper-item><navigator class="swiper-item" hover-class="none"><image src="{{v.img}}"></image></navigator></swiper-item></block></block></swiper></view><view class="entry"><block wx:for="{{info}}" wx:for-item="v" wx:for-index="k"><block><view class="row"><view class="title"><image src="{{v.video_icon}}"></image><text style="{{('color:'+v.video_color)}}">{{v.cate}}</text><navigator class="right" hover-class="none" url="{{'/pages/video/video?id='+v.subjects_id}}">更多></navigator></view><view class="video"><block wx:for="{{v.data}}" wx:for-item="val" wx:for-index="key"><block><navigator class="part" hover-class="none" url="{{'/pages/video/detail?id='+val.id}}"><view class="img"><image src="{{val.img}}" mode="scaleToFill"></image><view class="num">1203</view></view><view class="titles">3天带你玩转音标</view><view class="content">带你学习元音和辅音</view></navigator></block></block></view></view></block></block></view></view>