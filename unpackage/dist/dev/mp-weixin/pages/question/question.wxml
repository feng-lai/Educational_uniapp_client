<view><view class="top" id="top"><view class="title">{{title}}</view><view class="speed"><view class="left">{{subTitle}}</view><view class="right">{{''+(current+1)}}<text>{{"/"+num}}</text><view class="persent"><text style="{{('width:'+persent)}}"></text></view></view></view></view><swiper class="swiper" style="{{('height:'+height+'px')}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}" current="{{current}}" data-event-opts="{{[['change',[['changeType',['$event']]]]]}}" bindchange="__e"><block wx:for="{{info}}" wx:for-item="v" wx:for-index="k"><block><swiper-item style="overflow:auto;"><view class="content swiper-item"><view class="title">{{''+(k+1)+".("+(v.type==1?'单选题':'多选题')+")"+v.title+''}}</view><view class="option"><view data-event-opts="{{[['tap',[['chose',[k,1]]]]]}}" class="{{[v.check_A?'active row':'row']}}" bindtap="__e">A<text>|</text>{{v.option_A}}</view><view data-event-opts="{{[['tap',[['chose',[k,2]]]]]}}" class="{{[v.check_B?'active row':'row']}}" bindtap="__e">B<text>|</text>{{v.option_B}}</view><view data-event-opts="{{[['tap',[['chose',[k,3]]]]]}}" class="{{[v.check_C?'active row':'row']}}" bindtap="__e">C<text>|</text>{{v.option_C}}</view><view data-event-opts="{{[['tap',[['chose',[k,4]]]]]}}" class="{{[v.check_D?'active row':'row']}}" bindtap="__e">D<text>|</text>{{v.option_D}}</view></view></view></swiper-item></block></block></swiper><view class="nav"><view data-event-opts="{{[['tap',[['back']]]]}}" class="back" bindtap="__e">返回</view><block wx:if="{{current+1!=num}}"><view data-event-opts="{{[['tap',[['next']]]]}}" class="next" bindtap="__e">下一题</view></block><block wx:if="{{current+1==num}}"><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="next" bindtap="__e">提交</view></block></view></view>